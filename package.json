{
    "version": "0.0.1",
    "name": "pulsar-event-calendar",
    "description": "Given a list of selected events, allow the user to add and/or remove other dates.",
    "homepage": "https://github.com/jadu/pulsar-event-calendar",
    "author": "Paul Stanton <paul.stanton@jadu.co.uk>",
    "repository": {
        "type": "git",
        "url": "https://github.com/jadu/pulsar-event-calendar"
    },
    "bugs": {
        "url": "http://github.com/jadu/pulsar/issues"
    },
    "main": "index",
    "browserify": {
        "transform": [
            [
                "babelify",
                {
                    "presets": [
                        "es2015"
                    ]
                }
            ]
        ]
    },
    "scripts": {
        "build": "browserify index.js --outfile dist/EventCalendar.js",
        "jshint": "jshint .",
        "mocha": "mochify -r ./test/harness --recursive --reporter spec",
        "test": "npm run jshint && npm run mocha"
    },
    "dependencies": {
        "jquery": "~1",
        "clndr": "^1.4.7",
        "moment": "^2.15.2",
        "underscore": "^1.9.1"
    },
    "devDependencies": {
        "babel-core": "^6.26.3",
        "babel-preset-es2015": "^6.24.1",
        "babelify": "^7.3.0",
        "browserify": "^11.2.0",
        "chai": "^2.3.0",
        "grunt": "^1.0.3",
        "grunt-browserify": "^5.0.0",
        "grunt-contrib-watch": "^1.0.0",
        "grunt-sass": "^1.1.0",
        "jshint": "^2.8.0",
        "matchdep": "^2.0.0",
        "mochify": "^2.19.0",
        "sinon": "~1.15",
        "sinon-chai": "^2.7.0"
    },
    "license": "MIT"
}
